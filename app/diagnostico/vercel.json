{
    "version": 2,
    "buildCommand": null,
    "outputDirectory": ".",
    "rewrites": [
        {
            "source": "/",
            "destination": "/web/index.html"
        },
        {
            "source": "/trees",
            "destination": "/web/skill_trees.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        },
        {
            "source": "/data/(.*).json",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/json"
                },
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=86400"
                }
            ]
        },
        {
            "source": "/data/pruebas/(.*).xml",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/xml"
                }
            ]
        }
    ]
}