      <ns0:qti-simple-choice identifier="ChoiceC"><ns2:math><ns2:mfenced open="[" close="["><ns2:mn>1</ns2:mn><ns2:mn>3</ns2:mn></ns2:mfenced></ns2:math></ns0:qti-simple-choice>
      <ns0:qti-simple-choice identifier="ChoiceD"><ns2:math><ns2:mfenced open="]" close="]"><ns2:mn>1</ns2:mn><ns2:mn>3</ns2:mn></ns2:mfenced></ns2:math></ns0:qti-simple-choice>
    </ns0:qti-choice-interaction>
  </ns0:qti-item-body>
